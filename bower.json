{
  "name": "web-2-cv",
  "version": "0.0.1",
  "homepage": "https://github.com/levofski/web-2-cv",
  "authors": [
    "Chris Levy <chrisianlevy@yahoo.co.uk>"
  ],
  "description": "Web 2 CV Viewer/Manager",
  "main": [
    "public/css/main.css",
    "public/js/main.js"
  ],
  "keywords": [
    "cv"
  ],
  "license": "MIT",
