  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "public/vendor",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.3.15",
    "bootstrap": "~3.3.4",
    "angular-route": "~1.3.15",
    "angular-ui-router": "~0.2.13",
    "angular-xeditable": "~0.1.9",
    "angular-ui-ace": "bower",
    "angular-ui-bootstrap": "~0.12.1"
  },
  "overrides": {
    "ace-builds": {
      "main": [
        "src-min-noconflict/ace.js",
        "src-min-noconflict/mode-json.js",
        "src-min-noconflict/worker-json.js",
        "src-min-noconflict/mode-html.js",
        "src-min-noconflict/worker-html.js",
        "src-min-noconflict/theme-idle-fingers.js"
      ]
    },
    "angular-ui-bootstrap": {
      "main": [
        "src/modal/modal.js"
      ]
    }
  }
